{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "set NODE_ENV=test&& npm run build  && npm run testdb-up && jasmine-ts && npm run testdb-reset",
    "tsc": "tsc",
    "build": "npx tsc",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc.json src/**/*.ts --write",
    "jasmine": "jasmine",
    "devdb-up": "db-migrate up --config ./database.json --e dev",
    "devdb-reset": "db-migrate reset --config ./database.json --e dev",
    "testdb-up": "db-migrate --config ./database.json --e test up",
    "testdb-reset": "db-migrate reset --config ./database.json --e test"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.1",
    "db-migrate": "^1.0.0-beta.18",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^15.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "pg": "^8.7.3",
    "supertest": "^6.2.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.6",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
